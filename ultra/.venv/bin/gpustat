#!/home/dikshant/github/SMARTS/ultra/.venv/bin/python3.7
# EASY-INSTALL-ENTRY-SCRIPT: 'gpustat==0.6.0','console_scripts','gpustat'
__requires__ = 'gpustat==0.6.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('gpustat==0.6.0', 'console_scripts', 'gpustat')()
    )
